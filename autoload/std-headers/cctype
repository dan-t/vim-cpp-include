#ifndef CPPREFERENCE_CCTYPE_H
#define CPPREFERENCE_CCTYPE_H

namespace std {

int isalnum(int ch);
int isalpha(int ch);
#if CPPREFERENCE_STDVER>= 2011
int isblank(int ch);
#endif
int iscntrl(int ch);
int isdigit(int ch);
int isgraph(int ch);
int islower(int ch);
int isprint(int ch);
int ispunct(int ch);
int isspace(int ch);
int isupper(int ch);
int isxdigit(int ch);
int tolower(int ch);
int toupper(int ch);

} // namespace std

#endif // CPPREFERENCE_CCTYPE_H

